@using HaarlemFestival.Models;
@{
    ViewBag.Title = "AnswerContactmessage";
}
@model AnswerContactViewModel

<div class="container">
    @Html.EnumDropDownListFor(v => v.Category, "Select a Category.")

    @foreach(ContactMessage message in Model.Messages)
    {
        <div class="category-@((int)message.Regarding)">
            <div class=""
        </div>
    }
</div>